{
    "Html Boilerplate": {
        "prefix": "html",
        "body": [
            "<!DOCTYPE html>",
            "<html lang=\"en\" dir=\"ltr\">",
            "\t<head>",
            "\t\t<title>$1</title>",
            "\t\t<!-- Meta -->",
            "\t\t<meta charset=\"utf-8\">",
            "\t\t<meta name=\"description\" content=\"$2\">",
            "\t\t<meta name=\"robots\" content=\"index, follow\">",
            "\t\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">",
            "\t\t<!-- Open Graph -->",
            "\t\t<meta name=\"og:image\" content=\"./logo.png\">",
            "\t\t<!-- Links -->",
            "\t\t<link rel=\"icon\" href=\"./logo.png\">",
            "\t\t<link rel=\"apple-touch-icon\" href=\"./logo.png\">",
            "\t\t<link rel=\"stylesheet\" href=\"./css/main.css\">",
            "\t\t<link rel=\"stylesheet\" href=\"./css/cookie.css\">",
            "\t\t<link rel=\"stylesheet\" href=\"./css/index.css\">",
            "\t\t<script type=\"text/javascript\" src=\"./js/cookie.js\" defer></script>",
            "\t\t<script type=\"text/javascript\" src=\"./js/main.js\" defer></script>",
            "\t</head>",
            "\t<body>",
            "\t\t<header>",
            "\t\t\t$4",
            "\t\t</header>",
            "\t\t<main>",
            "\t\t\t$5",
            "\t\t</main>",
            "\t\t<footer>",
            "\t\t\t<h2>&copy; $CURRENT_YEAR - $CURRENT_YEAR <a href=\"./\" target=\"_self\">${3:Marcus Mortensen Jensen}</a>, All Rights Reserved.</h2>",
            "\t\t</footer>",
            "\t</body>",
            "</html>"
        ],
        "description": "Insert a basic HTML template"
    }
}